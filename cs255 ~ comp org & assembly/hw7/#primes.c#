#define EXTERN
#define MAX 1000000

#define SetBit(A,k) (A[(k/32)] |= (1 << (k%32)))
#define ClearBit(A,k) (A[(k/32)] &=  -(1 << (k%32)))
#define TestBit(A,k) (A[(k/32)] & (1 << (k%32)))

#include <stdio.h>
#include <stdlib.h>
